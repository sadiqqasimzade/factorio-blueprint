{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10"
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "ignore": ["node_modules"],
  "comments": false,
  "minified": true,
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
